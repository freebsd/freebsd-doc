#, no-wrap
msgid ""
".if defined(DOC_LANG) && !empty(DOC_LANG)\n"
"LANGUAGES=      ${DOC_LANG:S/,/ /g}\n"
".if  ${LANGUAGES:Men} == \"\"\n"
".warning \"Warning: cannot skip 'en'; adding it back\"\n"
"LANGUAGES+=\ten\n"
".endif\n"
".else\n"
"LANGUAGES=\t${ALL_LANGUAGES}\n"
".endif\n"
msgstr ""
".if defined(DOC_LANG) && !empty(DOC_LANG)\n"
