# Set up for NetBSD repo options.
#
# $FreeBSD: www/en/cgi/cvsweb.conf-netbsd,v 1.2 2000/09/03 18:46:25 knu Exp $

$ENV{'RCSLOCALID'} = 'NetBSD';                                                  
