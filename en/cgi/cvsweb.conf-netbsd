		   port-atari
		   port-bebox
		   port-cobalt
		   port-hp300
		   port-hpcmips
		   port-i386
		   port-luna68k
		   port-m68k
		   port-mac68k
		   port-macppc
		   port-mips
		   port-mvme68k
		   port-newsmips
		   port-news68k
		   port-next68k
		   port-ofppc
		   port-pc532
		   port-pmax
		   port-powerpc
		   port-prep
		   port-sgimips
		   port-sh3
		   port-sparc
		   port-sparc64
		   port-sun3
		   port-vax
		   port-x68k
		   security
		   standards
		   xsrc
		   y2k
		  );

