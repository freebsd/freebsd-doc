    <glossentry>
      <glossterm>UFS1</glossterm>
      <glosssee otherterm="ufs1-glossary">
    </glossentry>

    <glossentry>
      <glossterm>UFS2</glossterm>
      <glosssee otherterm="ufs2-glossary">
    </glossentry>

    <glossentry>
      <glossterm>UID</glossterm>
