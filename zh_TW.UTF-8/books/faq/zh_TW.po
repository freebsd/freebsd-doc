#. (itstool) path: step/screen
#: book.translate.xml:7220
#, no-wrap
msgid "<prompt>#</prompt> <userinput>cd /usr/src</userinput>"
msgstr ""

#. (itstool) path: step/para
#: book.translate.xml:7224
msgid "Compile the kernel:"
