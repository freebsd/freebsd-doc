#: article.translate.xml:88
#, no-wrap
msgid "<prompt>#</prompt> <userinput>/sbin/shutdown -r now</userinput>"
msgstr "<prompt>#</prompt> <userinput>/sbin/shutdown -r now</userinput>"

#. (itstool) path: sect1/para
#: article.translate.xml:91
