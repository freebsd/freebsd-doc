#: article.translate.xml:71
#, no-wrap
msgid "<prompt>#</prompt> <userinput>exit</userinput>"
msgstr "<prompt>#</prompt> <userinput>exit</userinput>"

#. (itstool) path: sect1/para
#: article.translate.xml:74
