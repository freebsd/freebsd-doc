#: book.translate.xml:13693
#, no-wrap
msgid ""
"USES=\t\tcmake kde:5 qt:5\n"
"USE_KDE=\tecm\n"
"USE_QT=\t\tcore buildtools_build qmake_build"
msgstr ""
"USES=\t\tcmake kde:5 qt:5\n"
"USE_KDE=\tecm\n"
"USE_QT=\t\tcore buildtools_build qmake_build"

